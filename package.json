{
  "name": "iitc-prime",
  "main": "app/app.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run:android": "ns run android",
    "build:android:release": "BUILD_TYPE=release ns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.snapshot --env.compileSnapshot --env.uglify",
    "build:android:beta": "BUILD_TYPE=beta ns build android --release --clean --key-store-path $KEYSTORE_PATH --key-store-password $KEYSTORE_PASSWORD --key-store-alias $KEYSTORE_ALIAS --key-store-alias-password $KEYSTORE_ALIAS_PASSWORD --env.snapshot --env.compileSnapshot --env.sourceMap",
    "build:android:debug": "BUILD_TYPE=debug ns build android --env.sourceMap",
    "run:ios": "ns run ios",
    "build:ios:release": "ns build ios --release --env.uglify",
    "build:ios:beta": "ns build ios",
    "clean": "ns clean",
    "lint": "ns build android --no-copy && echo 'Syntax check passed'",
    "postinstall": "patch-package",
    "version:patch": "npm version patch && npm run version:sync-ios",
    "version:minor": "npm version minor && npm run version:sync-ios",
    "version:major": "npm version major && npm run version:sync-ios",
    "version:sync-ios": "node scripts/sync-ios-version.mjs"
  },
  "dependencies": {
    "@bezlepkin/nativescript-keyboard-opening": "^1.0.1",
    "@nativescript-community/fonticon": "^3.0.0",
    "@nativescript-community/gps": "^3.1.10",
    "@nativescript-community/l": "^4.3.10",
    "@nativescript-community/ui-collectionview": "^6.0.4",
    "@nativescript-community/ui-label": "^1.3.33",
    "@nativescript-community/ui-material-bottomsheet": "^7.2.66",
    "@nativescript-community/ui-material-button": "^7.2.66",
    "@nativescript-community/ui-material-dialogs": "^7.2.71",
    "@nativescript-community/ui-material-ripple": "^7.2.71",
    "@nativescript-community/ui-material-switch": "^7.2.71",
    "@nativescript-community/ui-svg": "^0.2.11",
    "@nativescript-community/ui-webview": "^1.5.2",
    "@nativescript/appversion": "^2.0.0",
    "@nativescript/core": "~8.8.2",
    "@nativescript/theme": "^3.1.0",
    "@nstudio/nativescript-checkbox": "^2.0.5",
    "@triniwiz/nativescript-image-cache-it": "^7.5.0",
    "@triniwiz/nativescript-toasty": "^4.1.3",
    "lib-iitc-manager": "^1.9.3",
    "nativescript-appurl": "^1.3.5",
    "nativescript-clipboard": "^2.1.1",
    "nativescript-compass": "^1.0.1",
    "nativescript-inappbrowser": "^3.2.0",
    "nativescript-radio-dialog": "^1.0.4",
    "nativescript-vue": "^3.0.1",
    "nativescript-vue-devtools": "^1.5.1",
    "scored-fuzzysearch": "^1.0.5",
    "url-parse": "^1.5.10",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@nativescript/android": "^8.8.5",
    "@nativescript/ios": "8.9.2",
    "@nativescript/webpack": "~5.0.22",
    "@triniwiz/nativescript-socketio": "^4.0.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "simple-plist": "^1.3.1",
    "typescript": "^5.6.3",
    "util": "^0.12.5"
  }
}
